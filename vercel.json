{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/process.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/process/(.*)",
      "dest": "/api/process/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 